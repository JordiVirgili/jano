# Database configuration
# Options: "sqlite" or "postgres"
JANO_DB_TYPE=sqlite

# SQLite configuration (only if JANO_DB_TYPE=sqlite)
JANO_SQLITE_PATH=jano.db

# PostgreSQL configuration (only if JANO_DB_TYPE=postgres)
JANO_DB_HOST=localhost
JANO_DB_PORT=5432
JANO_DB_USER=postgres
JANO_DB_PASSWORD=postgres
JANO_DB_NAME=jano

# API Authentication
JANO_API_USERNAME=admin
JANO_API_PASSWORD=secure_password_here

# LLM Configuration
# Options: claude, openai, etc.
LLM_TYPE=claude

# Anthropic (Claude) Configuration
# Used for regular conversations
ANTHROPIC_DEFAULT_MODEL=claude-3-5-haiku-20241022
# Used for complex security tasks
ANTHROPIC_ADVANCED_MODEL=claude-3-7-sonnet-20250219

# OpenAI Configuration (if using OpenAI)
# OPENAI_API_KEY=your-openai-api-key-here
# OPENAI_MODEL=gpt-3.5-turbo  # Options: gpt-3.5-turbo, gpt-4, etc.